Discount = 0.95
TgherkinNrOfNOPRetries = 0
TempDir = ./resources/output/temp
MaxTime = 3.1536E7
StopGenerationOnFault = true
TgherkinReportIncludeImageRecognition = false
ExecuteActions = true
DrawWidgetTree = false
ApplyDefaultOnMismatch = true
UseRecordedActionDurationAndWaitTimeDuringReplay = false
ForceForeground = true
AlgorithmFormsFilling = false
GenerateTgherkinReport = true
FaultThreshold = 1.0E-9
DrawWidgetInfo = false
MaxReward = 9999999.0
StoreTgherkinReport = false
LogLevel = 1
VisualizeSelectedAction = true
TgherkinReportIncludeOCR = false
Delete = 
MyClassPath = resources/settings
ReplayRetryTime = 30.0
ProcessesToKillDuringTest = 
ShowSettingsAfterTest = true
ForceToSequenceLength = false
OutputDir = ./resources/output
UnattendedTests = false
TestGenerator = random
StartupTime = 10.0
ActionDuration = 0.1
NonReactingUIThreshold = 100
ContinueToApplyDefault = true
